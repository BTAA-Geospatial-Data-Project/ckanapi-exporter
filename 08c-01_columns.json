{

"UUID": {
      "pattern": "^id$",
      "unique": true,
      "case_sensitive": true
  },

"Type": {
      "pattern": ["^type$"]
  },
"Modified": {
      "pattern": ["^metadata_modified$"]
  },

"Creator": {
      "pattern": ["^tags$", "^display_name.*"]
  },

"Publisher": {
      "pattern": ["^organization$", "^title.*"]
  },

"Title": {
      "pattern": "^title$",
      "unique": true,
      "case_sensitive": true
  },

"DatasetURI": {
      "pattern": ["^name$"],
      "unique": true
  },

"Description": {
      "pattern": "^notes$",
      "unique": true,
      "case_sensitive": true
  },

"TopicCategory": {
      "pattern": ["^groups$", "^display_name$"],
      "deduplicate": true
  },


"DateIssued": {
      "pattern": ["^metadata_created$"],
      "deduplicate": true
  },


"Formats": {
      "pattern": ["^resources$", "^format$"]
  },


"Links": {
      "pattern": ["^resources$", "^url$"]
  }

}