{

"UUID": {
      "pattern": "^id$",
      "unique": true,
      "case_sensitive": true
  },

"Modified": {
      "pattern": ["^metadata_modified$"]
  },

"Creator": {
      "pattern": ["^extras$", "^dsOriginator.*"]
  },

"Publisher": {
      "pattern": ["^organization$", "^title.*"]
  },

"Title": {
      "pattern": "^title$",
      "unique": true,
      "case_sensitive": true
  },

"DatasetURI": {
      "pattern": ["^name$"],
      "unique": true
  },

"Description": {
      "pattern": "^notes$",
      "unique": true,
      "case_sensitive": true
  },

"TopicCategory": {
      "pattern": ["^groups$", "^display_name$"],
      "deduplicate": true
  },

"TemporalCoverage": {
      "pattern": ["^extras$", "^dsPeriodOfContent$"],
      "deduplicate": true
  },

"DateIssued": {
      "pattern": ["^metadata_created$"],
      "deduplicate": true
  },



"Spatial": {
      "pattern": ["^extras$", "^spatial$"]
  },

"Formats": {
      "pattern": ["^resources$", "^format$"],
      "deduplicate": true
  },

"MetadataLink": {
      "pattern": ["^extras$", "^dsMetadataUrl$"]
  },

"Links": {
      "pattern": ["^resources$", "^url$"]
  }

}